<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="activityService" class="cuit.math.activity.service.ActivityServiceImp">
	  <property name="activityDao" ref="activitytDao"> </property>
	</bean>
	
	<bean id="newsService" class="cuit.math.news.service.NewsServiceImp">
	  <property name="newsDao" ref="newsDao"> </property>
	</bean>
	
	<bean id="playerService" class="cuit.math.player.service.PlayerServiceImp">
	  <property name="playerDao" ref="playerDao"> </property>
	</bean>
	
	
</beans>